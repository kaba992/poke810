<main data-barba="container" data-barba-namespace="index">
  <h1>Pokemons#index</h1>
  <p>Find me in app/views/pokemons/index.html.erb</p>

    <% @pokemons.each do |pokemon| %>
      <%if current_user.id != pokemon.user_id  %>
        <div> <%= link_to pokemon.name, pokemon_path(pokemon) %> </div>
      <% end %>
    <% end %>
</main>
