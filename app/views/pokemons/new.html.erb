<h1>Pokemons#new</h1>
<p>Find me in app/views/pokemons/new.html.erb</p>
<div class="container">
  <%= simple_form_for(@pokemon) do |f| %>
    <%= f.input :name, prompt: "Choose a pokemon"%>
    <%= f.input :description%>
    <%= f.input :price %>

    <%= f.submit "Submit new pokemon", class: "morph_button" %>
  <% end %>
</div>
